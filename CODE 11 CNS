#include <stdio.h>
#include <math.h>

// Function to calculate log2(25!)
double log2_factorial(int n) {
    double log2_fact = 0.0;
    for (int i = 1; i <= n; i++) {
        log2_fact += log2(i);
    }
    return log2_fact;
}

int main() {
    int n = 25;
    double log2_keyspace = log2_factorial(n);

    printf("Possible number of Playfair cipher keys â‰ˆ 2^%.2f\n", log2_keyspace);
    printf("This is approximately %.0f possible keys.\n", pow(2, log2_keyspace));

    return 0;
}
