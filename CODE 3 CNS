#include <stdio.h>
#include <string.h>
#include <ctype.h>

#define SIZE 5

char matrix[SIZE][SIZE];

int exists(char *key, char ch) {
    for (int i = 0; key[i]; i++) {
        if (key[i] == ch) return 1;
    }
    return 0;
}

void generateMatrix(char *key) {
    char temp[26] = {0};
    int idx = 0;
    for (int i = 0; key[i]; i++) {
        char ch = toupper(key[i]);
        if (ch == 'J') ch = 'I';  // Combine I and J
        if (!exists(temp, ch) && isalpha(ch)) {
            temp[idx++] = ch;
        }
    }

    for (char ch = 'A'; ch <= 'Z'; ch++) {
        if (ch == 'J') continue;  // J merged with I
        if (!exists(temp, ch)) {
            temp[idx++] = ch;
        }
    }

    idx = 0;
    for (int i = 0; i < SIZE; i++)
        for (int j = 0; j < SIZE; j++)
            matrix[i][j] = temp[idx++];
}
